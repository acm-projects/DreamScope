{
  "expo": {
    "name": "dreamscope-mobile",
    "slug": "dreamscope-mobile",
    "scheme": "notjust-dream",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "Frontend/images/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "Frontend/images/logo-trans.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C123F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "Frontend/images/icon.png",
        "backgroundColor": "#2C123F"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "Frontend/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}