{
  "expo": {
    "name": "dreamscope-mobile",
    "slug": "dreamscope-mobile",
    "scheme": "notjust-dream",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "Frontend/images/icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "Frontend/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C123F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "Frontend/images/adaptive-icon.png",
        "backgroundColor": "#2C123F"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "Frontend/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./Frontend/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
